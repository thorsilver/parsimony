/* c <- a xor b */

/* 
a: inp int
b: inp int
c: out int
*/

func xor(a, b, c) {
    zero(c);

    if (a) {
        if (b) {
            return;
        }
        one(c);
        return;
    }
    if (b) {
        one(c);
        return;
    }
    return;
}

