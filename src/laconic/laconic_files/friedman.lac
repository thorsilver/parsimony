                        hasAFree(graph, edges, numVertices, eightKN, eightKNRFacFac, k, n, r, h1, h2, h3, h4, h5, 
                            h6, h7, h8, hl1, hl2, hl3, hl4, hl5, hl6, hl7, hl8, hll, h9);
                            
                        if (!h9) {
                            halt;
                        }        
                    }
                    
                
                    /* we must make a new edge-set */
                
                    incrementList(edges, h1, h2, h4, h3, hl1);
                    edges = hl1;
                } 
                incr(r);
            }
            incr(n);
        }
        incr(k);
    }
    incr(maxKNR);
}

