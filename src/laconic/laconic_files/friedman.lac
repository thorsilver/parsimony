h: hold int
x: out in
*/


func sum(l, i, h, x) {
    doubleZero(i, x);
    
    lessThanLength(i, l, h); 
    
    while (h) {
        index(l, i, h);
        x = x + h;
        incrAndLessThanLength(i, l, h);
    }
    
    return;
}


