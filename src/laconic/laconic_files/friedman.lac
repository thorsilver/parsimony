h2: hold int
hl1: hold list
hl2: hold list
output: out list2
*/

func ush2(l2, eightKNRFacFac, i, h1, h2, hl1, hl2, output) {
    empty2(output);
    
    zeroAndLessThanLength2(i, l2, h1);
    while (h1) {
        index2(l2, i, hl1);
        ush(hl1, eightKNRFacFac, h1, h2, hl2);
        
        append2(output, hl2);
        
        incr(i);
        lessThanLength2(i, l2, h1);
    }
    
    return;
}

