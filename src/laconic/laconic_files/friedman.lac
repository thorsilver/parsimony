/*----------------------------------------------------------*/

/* x ltrlex y if and only if x = y or x_i < y_i where i is least such 
that x_i != y_i 

x: inp list
y: inp list
i: hold int
h1: int
h2: int
output: out int
*/

func ltrlex(x, y, i, h1, h2, output) {
    
    doubleZero(i, output);
    
    /* while ((i < (# x)) & (i < (# y))) */
    lessThanLengthTwice(i, x, y, h1, h2);
    
    while (h1 & h2) {
        
        indexTwice(x, y, i, h1, h2);
    
        if (h1 < h2) {
            one(output);
            return;
        }
        
        if (h1 > h2) {
            return;
        }
        
        incr(i);
        
        lessThanLengthTwice(i, x, y, h1, h2);
    }
    
    notSameLength(x, y, h1);
    
    if (h1) {
        return;
    }
    
    output = 1;
    return;
}

