/*----------------------------------------------------------*/

/*
l2: inp list2
l: inp list
i: hold int
h: hold int
output: out list2;
*/

func map2(l2, l, i, h, output) {
    empty2(output);
    zeroAndLessThanLength2(i, l2, h);
    
    while(h) {
        index(l, i, h);
        
        ifelse (h) {
            index2(l2, i, h);
            append2(output, h);
            incr(i);
        }
        
        {   
            incr(i);
        }
        
        lessThanLength(i, l, h);
    }
    
    return;
}

